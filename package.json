{
  "name": "vscode-ctrl-e",
  "displayName": "Ctrl+E Selection to REPL",
  "description": "Adds Ctrl+E keyboard shortcut to send selection to REPL",
  "version": "0.0.1",
  "publisher": "khwbagm",
  "repository": {
    "type": "git",
    "url": "https://github.com/khwbagm/vscode-ctrl-e"
  },
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Keymaps"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "keybindings": [
      {
        "key": "ctrl+e",
        "command": "editor.debug.action.selectionToRepl"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/node": "16.x",
    "typescript": "^4.9.4"
  }
} 